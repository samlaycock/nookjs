{
  "name": "interpreter",
  "private": true,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "lint": "oxlint . --type-aware",
    "fmt": "oxfmt .",
    "typecheck": "tsc --noEmit",
    "build": "tsdown",
    "benchmark": "bun run benchmark.ts"
  },
  "dependencies": {
    "meriyah": "7.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "oxfmt": "0.26.0",
    "oxlint": "1.41.0",
    "oxlint-tsgolint": "0.11.1",
    "tsdown": "0.20.0-beta.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
