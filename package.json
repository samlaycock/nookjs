{
  "name": "nookjs",
  "version": "0.0.9",
  "description": "A fast, standards-compliant JavaScript/TypeScript parser and interpreter written in TypeScript.",
  "homepage": "https://nookjs.dev",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "lint": "oxlint . --type-aware",
    "fmt": "oxfmt .",
    "typecheck": "tsc --noEmit",
    "build": "tsdown",
    "benchmark": "bun run benchmark.ts",
    "benchmark:parse": "bun run benchmark-parse.ts",
    "benchmark:parse:micro": "bun run benchmark-parse-micro.ts",
    "benchmark:parse:profile": "bun run benchmark-parse-profile.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "meriyah": "7.0.0",
    "oxfmt": "0.26.0",
    "oxlint": "1.41.0",
    "oxlint-tsgolint": "0.11.1",
    "tsdown": "0.20.0-beta.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
